{
  "name": "@more/plop",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules && rm -f tsconfig.tsbuildinfo",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint",
    "fix:lint": "eslint --fix",
    "generate:package": "plop --plopfile scripts/package.ts",
    "generate:app": "plop --plopfile scripts/app.ts"
  },
  "dependencies": {
    "plop": "^4.0.1"
  },
  "devDependencies": {
    "@more/configs": "workspace:*",
    "eslint": "^9.15.0"
  }
}
