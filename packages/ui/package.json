{
  "name": "@more/ui",
  "version": "1.0.0",
  "sideEffects": false,
  "private": true,
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/",
    "loader/"
  ],
  "exports": {
    ".": {
      "import": "./dist/more-ui/more-ui.esm.js",
      "require": "./dist/more-ui/more-ui.cjs.js"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./hydrate": {
      "types": "./hydrate/index.d.ts",
      "import": "./hydrate/index.mjs",
      "require": "./hydrate/index.js",
      "default": "./hydrate/index.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf .stencil dist loader storybook-static www",
    "format": "yarn lint --fix",
    "lint": "eslint",
    "build": "stencil build",
    "dev": "stencil build --dev --watch --serve",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@more/configs": "workspace:*",
    "@stencil/core": "^4.31.0",
    "@stencil/react-output-target": "^1.0.2",
    "@stencil/storybook-plugin": "^0.0.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/html": "^8.6.14",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "storybook": "^8.6.14",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
